@using FinalProject.Models@model FinalProject.Models.DynamicTemplate@{    ViewBag.Title = "Upload CSV";}<h2>Upload CSV File</h2><form action="@Url.Action("AddCsv", "DynamicTemplate")" method="post" enctype="multipart/form-data">    <label>Template Name:</label>    <input type="text" name="templateName" required class="form-control" />    <label>Select CSV File:</label>    <input type="file" name="file" required class="form-control" accept=".csv" />    <br />    <button type="submit" class="btn btn-success">Upload & Create Table</button></form>@if (ViewBag.Message != null){<script>        alert("@ViewBag.Message");</script>}