@using FinalProject.Models@model FinalProject.Models.DynamicTemplate@{    ViewBag.Title = "Upload CSV";}<h2>Upload CSV File</h2><form action="@Url.Action("AddCsv", "DynamicTemplate")" method="post" enctype="multipart/form-data">    <label>Template Name:</label>    <input type="text" name="TemplateName" required class="form-control" />    <label>Domain:</label>    <input type="text" name="Domain" required class="form-control" />    <label>Category:</label>    <input type="text" name="Category" required class="form-control" />    <label>School Year:</label>    <input type="text" name="SchoolYear" required class="form-control" />    <label>Roles:</label>    <input type="text" name="Roles" required class="form-control" />    <label>Status ID:</label>    <input type="number" name="StatusId" required class="form-control" />    <label>Select CSV File:</label>    <input type="file" name="file" required class="form-control" accept=".csv" />    <br />    <button type="submit" class="btn btn-success">Upload & Create Table</button></form>@if (ViewBag.Message != null){    <script>    alert("@ViewBag.Message");    </script>}